{
    "name": "nhlgram",
    "version": "0.1.0",
    "description": "Unofficial video feed of current NHL season",
    "browserslist": [
        "last 2 versions"
    ],
    "scripts": {
        "dev": "node \"./src/functions/devServer.js\"",
        "preserve": "npm run build-all && npm run copy-deps && npm run install-deps",
        "serve": "cross-env NODE_ENV=production firebase serve",
        "predeploy": "npm run build-all && npm run copy-deps",
        "deploy": "cross-env NODE_ENV=production firebase deploy",
        "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
        "build-all": "npm run build-public && npm run build-funcs && npm run build-next",
        "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
        "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
        "build-next": "next build \"src/app/\"",
        "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
        "install-deps": "cd \"dist/functions\" && npm i"
    },
    "engines": {
        "node": "12"
    },
    "author": "Martin Juzl",
    "license": "MIT",
    "dependencies": {
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-sass": "^1.0.1",
        "axios": "^0.19.0",
        "babel-plugin-styled-components": "^1.10.7",
        "dotenv": "^8.0.0",
        "firebase": "^7.14.3",
        "firebase-admin": "^8.12.1",
        "firebase-functions": "^3.6.1",
        "fs-extra": "^8.1.0",
        "moment": "^2.24.0",
        "next": "^9.4.0",
        "next-images": "^1.1.1",
        "next-redux-wrapper": "^6.0.0",
        "next-routes": "^1.4.2",
        "nodemailer": "^6.2.1",
        "normalize.css": "^8.0.1",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-ga": "^2.7.0",
        "react-id-swiper": "^3.0.0",
        "react-intersection-observer": "^8.25.2",
        "react-lazy-load-image-component": "^1.4.1",
        "react-parallax": "^2.2.4",
        "react-redux": "^7.2.0",
        "react-responsive-modal": "^4.0.1",
        "react-spring": "^8.0.27",
        "react-sticky-el": "^1.1.0",
        "react-string-replace": "^0.4.4",
        "react-swipeable-views": "^0.13.3",
        "recompose": "^0.30.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "scrolldir-observable": "^1.0.0",
        "styled-components": "^5.1.0",
        "swiper": "^5.3.8",
        "use-debounce": "^3.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "autoprefixer": "^9.5.0",
        "cpx": "^1.5.0",
        "cross-env": "^5.2.0",
        "firebase-tools": "^6.12.0",
        "node-sass": "^4.14.1",
        "rimraf": "^2.6.0"
    }
}
